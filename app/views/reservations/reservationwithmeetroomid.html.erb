<%= form_for @reservation, url: reservationid_path(@meetroom.id) do |r| %>
<%= link_to "Back", meetroom_path(@meetroom.id) %> <br>
<hr>
<%= r.date_select :dateBoth, start_year: Date.current.year, end_year: Date.current.year %> <br>
<hr>
<%= r.time_select :timeStart %> -
<%= r.time_select :timeEnd %> <br>


<%= r.submit 'Reserve meetroom', data: {confirm: "Are you sure?"} %>
  <hr>

<% end %>
