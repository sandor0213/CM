<%= form_for @reservation, url: reservationid_path(@meetroom.id) do |r| %>
<%= @meetroom.title %> <br>
<%= @meetroom.id %> <br>
<%= r.text_field :userName %> <br>

<hr>
<%= r.datetime_select :timeDateStart, start_year: Date.current.year, end_year: Date.current.year %> <br>
<%= r.datetime_select :timeDateEnd, start_year: Date.current.year, end_year: Date.current.year %>

<hr>


<%= r.submit 'Res meetroom' %>
  <hr>

<% end %>
