<%= @meetroom.title  %> <br>

<%= link_to "Reservation", reservationwithmeetroomid_path(@meetroom.id) %> <br>

| <%= link_to "Edit", edit_meetroom_path(@meetroom.id) %> <br> | <%= link_to "Delete", meetroom_path(@meetroom.id), method: :delete, data: {confirm: "Are you sure?"} %> | <br>

<% @reservations.each do |r| %>
<% if r.meetroom_id == @meetroom.id %>
<%= r.meetroom_id %>
<%= r.userName %>
<%= r.timeStart %> <br>
<%= r.timeEnd %> <br>
<%= link_to r.timeEnd.strip("%Y %m %d") %>
<%= "13-12-2012".to_time  %>
<hr>
<%= r.timeStart.to_time %> - <%= r.timeEnd.to_time %>

<hr>
<%= link_to "Delete", reservation_path(r.id), method: :delete, data: {confirm: "Are you sure?"} %>
<hr>
<% end %>


	<% end %>